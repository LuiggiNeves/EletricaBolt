RewriteEngine On

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

RewriteRule ^/(.*)$ index.php?url=$1 [NC,L]
RewriteRule ^login/(.*)$ app/view/pages/login.php?url=$1 [NC,L]

RewriteRule ^painel/(.*)$ app/view/pages/paineis/admin/painel.php?url=$1 [NC,L]
RewriteRule ^categorias/(.*)$ app/view/pages/paineis/admin/categorias.php?url=$1 [NC,L]
RewriteRule ^produtos/(.*)$ app/view/pages/paineis/admin/produtos.php?url=$1 [NC,L]
RewriteRule ^produtos-visualizar/(.*)$ app/view/pages/paineis/admin/produtos_visualizar.php?url=$1 [NC,L]

RewriteRule ^eb/produtos-visualizar/(.*)$ app/view/pages/produto-preview.php?url=$1 [NC,L]